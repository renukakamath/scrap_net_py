pragma solidity >=0.4.22 <0.9.0;

contract vehicleinfo{
uint public v_count=0;
    mapping(uint => vehicle) public vehs;
    struct vehicle{        
       uint id;
	  uint bid;
        uint uid;
        uint vehid;
	  string vehinfo;
        string date;
    }
    function add_info(uint _bid,uint _uid,uint _vehid,string memory _vehinfo,string memory _date)public{
        v_count++;
     vehs[v_count]=vehicle(v_count,_bid,_uid,_vehid,_vehinfo,_date);
    }


    
}
